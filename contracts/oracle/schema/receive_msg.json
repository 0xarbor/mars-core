{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ReceiveMsg",
  "anyOf": [
    {
      "description": "Deposit the sent cw20 tokens",
      "type": "object",
      "required": [
        "deposit_cw20"
      ],
      "properties": {
        "deposit_cw20": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Repay the sent cw20 tokens",
      "type": "object",
      "required": [
        "repay_cw20"
      ],
      "properties": {
        "repay_cw20": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Use the sent cw20 tokens to pay off a specified user's under-collateralized cw20 loan",
      "type": "object",
      "required": [
        "liquidate_cw20"
      ],
      "properties": {
        "liquidate_cw20": {
          "type": "object",
          "required": [
            "collateral_asset",
            "debt_asset_address",
            "receive_ma_token",
            "user_address"
          ],
          "properties": {
            "collateral_asset": {
              "description": "Details for collateral asset",
              "allOf": [
                {
                  "$ref": "#/definitions/Asset"
                }
              ]
            },
            "debt_asset_address": {
              "description": "Token address of the debt asset",
              "type": "string"
            },
            "receive_ma_token": {
              "description": "Sends maAsset to liquidator if true and underlying collateral asset if false",
              "type": "boolean"
            },
            "user_address": {
              "description": "The address of the borrower getting liquidated",
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "Asset": {
      "description": "Represents either a native asset or a cw20. Meant to be used as part of a msg in a contract call and not to be used internally",
      "anyOf": [
        {
          "type": "object",
          "required": [
            "cw20"
          ],
          "properties": {
            "cw20": {
              "type": "object",
              "required": [
                "contract_addr"
              ],
              "properties": {
                "contract_addr": {
                  "type": "string"
                }
              }
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "native"
          ],
          "properties": {
            "native": {
              "type": "object",
              "required": [
                "denom"
              ],
              "properties": {
                "denom": {
                  "type": "string"
                }
              }
            }
          },
          "additionalProperties": false
        }
      ]
    }
  }
}
